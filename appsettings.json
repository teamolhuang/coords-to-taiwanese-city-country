{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "SigningKey": "change-this-example-key-or-i-will-be-angry",
    "ExpireMinutes": 60 
  },
  "Throttling": {
    // API 執行限制的寬容期間。預設 1 代表每次執行會保持在記錄中 1 秒
    "WindowDurationSeconds": 1,
    // 在寬容期間最多可以執行幾次。預設 100 代表 100 次
    "MaxExecutionCount": 100
  }
}
